{
  "name": "generator-jhipster-full-page",
  "version": "0.0.1",
  "description": "JHipster module that creates a full page with client and/or server side, using data from/to server, custom form and tables.",
  "keywords": [
    "yeoman-generator",
    "jhipster-module",
    "jhipster-6"
  ],
  "homepage": "https://github.com/amanganiello90/generator-jhipster-full-page",
  "author": {
    "name": "Angelo Manganiello",
    "email": "angelo.mang@libero.it",
    "url": "https://github.com/amanganiello90"
  },
  "files": [
    "generators"
  ],
  "main": "generators/app/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amanganiello90/generator-jhipster-full-page.git"
  },
  "dependencies": {
    "chalk": "3.0.0",
    "generator-jhipster": "6.7.0",
    "lodash": "4.17.15",
    "mkdirp": "0.5.1",
    "semver": "7.1.1",
    "shelljs": "0.8.3",
    "yeoman-generator": "4.4.0"
  },
  "devDependencies": {
    "ejs-lint": "1.0.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-prettier": "3.1.2",
    "fs-extra": "8.1.0",
    "glob": "7.1.6",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "mocha": "6.2.2",
    "prettier": "1.19.1",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "2.0.0"
  },
  "scripts": {
    "ejs-lint": "mocha test test/ejslint.js",
    "prettier:format": "prettier --write \"{,**/}*.{js,json,md,yml}\"",
    "pretest": "npm run lint",
    "lint": "eslint . && npm run ejs-lint",
    "lint-fix": "eslint . --fix",
    "release": "git push && git push --tags && npm publish",
    "release:patch": "npm version patch -a -m \"Update to %s\" && npm run release",
    "release:minor": "npm version minor -a -m \"Update to %s\" && npm run release",
    "release:major": "npm version major -a -m \"Update to %s\" && npm run release",
    "test": "mocha test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,yml,md}": [
      "prettier --write",
      "npm run lint",
      "git add ."
    ]
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/amanganiello90/generator-jhipster-full-page/issues"
  }
}
